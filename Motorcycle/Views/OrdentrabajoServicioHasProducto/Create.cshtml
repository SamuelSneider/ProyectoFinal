@model Motorcycle.Models.OrdentrabajoServicioHasProducto

@{
    Layout = null;
    ViewData["Title"] = "Create";
}
<div class="container">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdServicioOrdenTrabajo" class="control-label"></label>
                <select asp-for="IdServicioOrdenTrabajo" class ="form-control" asp-items="ViewBag.IdServicioOrdenTrabajo"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdProducto" class="control-label"></label>
                <select asp-for="IdProducto" class ="form-control" asp-items="ViewBag.IdProducto"></select>
            </div>
            <div class="form-group">
                <label asp-for="PrecioProductoOrdentrabajoServicioHasProductos" class="control-label"></label>
                <input asp-for="PrecioProductoOrdentrabajoServicioHasProductos" class="form-control" />
                <span asp-validation-for="PrecioProductoOrdentrabajoServicioHasProductos" class="text-danger"></span>
            </div>
            <div class="form-group">
               <input type="submit" value="Crear" class="btn btn-custom-warning btn-sm mr-2" />
                <button type="button" class="btn btn-primarys btn-sm mr-2" data-bs-dismiss="modal">Cancelar</button>
            </div>
        </form>
</div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
